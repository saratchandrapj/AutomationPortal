@model List<AutomationPortal.Models.FilesToOpen>

@{
    ViewBag.Title = "OpenSprintFolder";
}

<h2>OpenSprintFolder</h2>

@foreach (var f in @Model)
{
	<ul>
		<li><a href="../Home/OpenFile?fileName=@f.path">@f.fileName</a></li>
		@*<li>Sprint 2</li>
			<li>Sprint 3</li>
			<li>Sprint 4</li>
			<li>Sprint 5</li>*@
	</ul>
}

